<% provide(:title, '新規登録') %>

<%= render layout: 'layouts/form' do %>
  <h1>新規登録</h1>
  <%= form_for [ :admin, @user ] do |f| %>
    <%= render 'form', f: f %>
    <%= f.submit '登録', class: "btn btn-primary" %>
    <%= link_to 'キャンセル', :admin_users, class: "btn btn-default" %>
  <% end %>
<% end %>
