<% provide(:title, '投稿の編集') %>
<h1>投稿の編集</h1>

<div class="row">
  <div class="col-md-6 col-md-offset-3">
    <%= form_for @post do |f| %>
      <%= render 'form', f: f %>
      <%= f.submit '更新', class: "btn btn-primary" %>
      <%= link_to 'キャンセル', :posts, class: "btn btn-default" %>
    <% end %>
  </div>
</div>
