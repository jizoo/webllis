<% provide(:title, '投稿') %>
<h1>投稿</h1>

<div class="row">
  <div class="col-md-6 col-md-offset-3">
    <%= form_for [ :editor, @post ] do |f| %>
      <%= render 'form', f: f %>
      <%= f.submit '投稿', class: 'btn btn-primary' %>
      <%= link_to 'キャンセル', :editor_posts, class: 'btn btn-default' %>
    <% end %>
  </div>
</div>
